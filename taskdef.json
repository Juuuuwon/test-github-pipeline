
{
    "family": "token-td",
    "containerDefinitions": [
        {
            "name": "app",
            "image": "<IMAGE_NAME>",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "app-port",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "linuxParameters": {
                "capabilities": {
                    "add": [
                        "SYS_PTRACE"
                    ],
                    "drop": []
                }
            },
            "logConfiguration": {
                "logDriver": "awsfirelens",
                "options": {},
                "secretOptions": []
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8080/healthcheck || exit 1"
                ],
                "interval": 5,
                "timeout": 60,
                "retries": 3,
                "startPeriod": 5
            },
            "systemControls": []
        },
        {
            "name": "log_router",
            "image": "public.ecr.aws/u6l3s4a6/holyshit:myflue",
            "cpu": 0,
            "portMappings": [],
            "essential": true,
            "environment": [
                {
                    "name": "INIT_SCRIPT",
                    "value": "Y2htb2QgLVIgNzc3IC92YXIvbG9nL2FwcDs="
                },
                {
                    "name": "FLUENT_BIT_CONFIG",
                    "value": ""
                },
                {
                    "name": "FLUENT_BIT_PARSERS",
                    "value": ""
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "user": "0",
            "readonlyRootFilesystem": false,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/ecs-aws-firelens-sidecar-container",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "firelens"
                },
                "secretOptions": []
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:2020/api/v1/health || exit 1"
                ],
                "interval": 5,
                "timeout": 2,
                "retries": 3,
                "startPeriod": 5
            },
            "systemControls": [],
            "firelensConfiguration": {
                "type": "fluentbit"
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::972209100553:role/ecs-task-role",
    "executionRoleArn": "arn:aws:iam::972209100553:role/ecs-task-execution-role",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "memory": "200",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "enableFaultInjection": false
}